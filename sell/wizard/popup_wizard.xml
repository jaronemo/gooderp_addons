<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="popup_wizard_form" model="ir.ui.view">
            <field name="name">popup.wizard.form</field>
            <field name="model">popup.wizard</field>
            <field name="arch" type="xml">
            	<form>
            		<field name='msg' readonly='1'/>
                    <footer>
                        <button name="button_ok" type="object" string="确定" class="oe_highlight"/>
                        or
                        <button string="取消" class="oe_link" special="cancel"/>
                    </footer>
            	</form>
            </field>
        </record>
    </data>
</openerp>